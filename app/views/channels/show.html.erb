<div class="row">
	<div class="bs-example">
		<% if notice.present? %>
			<div class="alert alert-warning fade in">
		    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
		    <%= notice %>
		  </div>
	  <% end %>
		<table class="table table-striped">
      <thead>
        <tr>
          <th width="20%">Channel Name</th>
          <th><%= @channel.name %></th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><b>IP Address</b></td>
          <td><%= @channel.ip_address %></td>
        </tr>
        <tr>
          <td><b>Location</b></td>
          <td><%= @channel.location.name %></td>
        </tr>
        <tr>
          <td><b>Status</b></td>
          <td><%= @channel.status %></td>
        </tr>
        <tr>
          <td><b>Port</b></td>
          <td><%= @channel.port %></td>
        </tr>
        <tr>
          <td><b>Conn Type</b></td>
          <td><%= @channel.conn_type %></td>
        </tr>
      </tbody>
    </table>

		<%= link_to 'Edit', edit_channel_path(@channel) %> |
		<%= link_to 'Back', channels_path %>
	</div>
</div>